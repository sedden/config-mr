[DEFAULT]
git_gc = git gc "$@"
git_update = git pull --ff-only "$@"
jobs = 5
include = 
	   for f in ~/.config/mr/lib.d/* ; do
	       test -f $f && echo "lib = . $f"
	   done
	   cat ~/.config/mr/config.d/*

[.config/vcsh/repo.d/mr.git]
checkout = vcsh clone git@github.com:sedden/config-mr.git mr

[.config/vcsh/repo.d/git.git]
checkout = vcsh clone git@github.com:sedden/config-git.git git

[.config/vcsh/repo.d/zsh.git]
checkout = vcsh clone git@github.com:sedden/config-zsh.git zsh

[.config/vcsh/repo.d/tmux.git]
checkout = vcsh clone git@github.com:sedden/config-tmux.git tmux

[.config/vcsh/repo.d/vim.git]
checkout = vcsh clone git@github.com:sedden/config-vim.git vim

[.config/vcsh/repo.d/emacs.git]
checkout = vcsh clone git@github.com:sedden/config-emacs.git emacs

[.config/vcsh/repo.d/atom.git]
checkout = vcsh clone git@github.com:sedden/config-atom.git atom
skip = ! osx

[.config/vcsh/repo.d/vscode.git]
checkout = vcsh clone git@github.com:sedden/config-vscode.git vscode
skip = ! osx

[.jenv]
checkout = git clone 'https://github.com/gcuisinier/jenv.git' .jenv
push = :

[.pyenv]
checkout = git clone 'https://github.com/pyenv/pyenv.git' .pyenv
push = :

[.rbenv]
checkout = git clone 'https://github.com/rbenv/rbenv.git' .rbenv
push = :

[.rbenv/plugins/ruby-build]
checkout = git clone 'https://github.com/rbenv/ruby-build.git' ruby-build
push = :
